    "remove-local": "graph remove --node http://localhost:8020/ nestfi-local"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.56.0",
    "@graphprotocol/graph-ts": "0.31.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.2"
  }
}
